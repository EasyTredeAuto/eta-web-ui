{
    "plugins": [
        "eslint-config-prettier",
        "eslint.autoFixOnSave"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es6": true
    },
    "rules": {
        "semi": "error",
        "prefer-const": "error"
    }
}